<h1>Listing questions</h1>

<table>
  <thead>
    <tr>
      <th>Quest</th>
      <th>A</th>
      <th>B</th>
      <th>C</th>
      <th>D</th>
      <!-- <th>Answer</th> -->
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @questions.each do |question| %>
      <tr>
        <td><%= question.quest %></td>
        <td><%= question.a %></td>
        <td><%= question.b %></td>
        <td><%= question.c %></td>
        <td><%= question.d %></td>
        <!-- <td><%= question.answer %></td> -->
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>


